{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/health",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/cron/refresh-analytics",
      "schedule": "0 * * * *"
    }
  ]
}
